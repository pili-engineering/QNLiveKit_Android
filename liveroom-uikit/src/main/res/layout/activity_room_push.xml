<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true"
    tools:context=".RoomPushActivity">

    <com.qncube.liveuikit.component.RoomBackGroundView
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.nucube.rtclive.QPushTextureView
        android:id="@+id/preTextureView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.qncube.uikitpk.PKPreView
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.qncube.uikitpk.PKCoverView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <com.qncube.kitlivepre.LivePreView
        android:id="@+id/mLivePreView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.qncube.liveuikit.component.RoomCoverViewPage
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</FrameLayout>